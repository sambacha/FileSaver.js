{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "ie > 9"
        ]
      }
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-modules-umd", {
      "globals": {
        "es6-promise": "Promise"
      }
    }]
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  }
}
